#include <stdio.h>
#include <cs50.h>

int main(void)
{
    int n, i, j;
    do
    {
    do
    {
        n= get_int("enter height: ");
    }
    while (n<1);
    }
    while (n>8);
    for (i=1; i<=n; i++)
    {
     for (j=1; j<=8-i; j++)
     {
         printf(" ");
     } 
     for (int k=0; k<=8-j; k++)
     {
         printf("#");
     }
        printf("  ");
     for (int k=0; k<=8-j; k++)
     {
         printf("#");
     }
        printf("\n");
    }
    printf("VAMOS CARAJO\n");
}
