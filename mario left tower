
#include <stdio.h>
#include <cs50.h>

int main(void)
{
    int n;
    do
    {
    do
    {
        n= get_int("enter height: ");
    }
    while (n<1);
    }
    while (n>8);
    for (int i=1; i<=n; i++)
    {
     for (int j=1; j<=i; j++)
     {
         printf("#");
     }
     printf("\n");
    }

}
